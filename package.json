{
  "name": "crystal-ball.github.io",
  "version": "1.0.0",
  "description": "Crystal Ball Projects resources and documentation ðŸ”®âœ¨",
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "keywords": [
    "crystal-ball",
    "awesomeness"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/crystal-ball.github.io.git"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "NODE_ENV=test eslint --format=pretty **/*.js",
    "create-github-labels": "node ./scripts/create-github-labels.js",
    "format": "prettier --write \"*.{js,md}\" \"{.github,scripts}/**/*.{js,json,md}\""
  },
  "dependencies": {
    "inquirer": "6.5.0",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@crystal-ball/commit-semantics": "1.1.0",
    "@crystal-ball/prettier-base": "1.2.0",
    "eslint-config-eloquence": "12.0.0",
    "husky": "3.0.0"
  },
  "prettier": "@crystal-ball/prettier-base",
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-adapter"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  }
}
