{
  "name": "crystal-ball.github.io",
  "version": "1.0.0",
  "description": "Organization shared resources and documentation",
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "keywords": [
    "crystal-ball",
    "awesomeness"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/crystal-ball.github.io.git"
  },
  "scripts": {
    "commit": "git cz",
    "test": "npm run lint",
    "lint": "eslint *.js",
    "labels": "node ./scripts/github-labels.js",
    "format": "prettier --write \"*.{js,md}\" \"{.github,scripts}/**/*.{js,json,md}\""
  },
  "dependencies": {
    "inquirer": "~6.2.1",
    "node-fetch": "~2.3.0"
  },
  "devDependencies": {
    "commitizen": "^3.0.5",
    "cz-customizable": "^5.3.0",
    "eslint-config-eloquence": "^9.5.1",
    "prettier": "^1.15.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
