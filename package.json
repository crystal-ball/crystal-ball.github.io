{
  "name": "crystal-ball.github.io",
  "version": "1.0.0",
  "description": "Crystal Ball Projects resources and documentation ðŸ”®âœ¨",
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "keywords": [
    "crystal-ball",
    "awesomeness"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/crystal-ball.github.io.git"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack serve --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty . --ignore-pattern 'src/vendor.js'",
    "test:unit": "jest scripts",
    "create-github-labels": "node ./scripts/create-github-labels.mjs",
    "format": "prettier --write . !coverage"
  },
  "dependencies": {
    "@babel/runtime": "7.12.13",
    "@mdx-js/react": "1.6.22",
    "classnames": "2.2.6",
    "core-js": "3.8.3",
    "feather-icons": "4.28.0",
    "inquirer": "7.3.3",
    "node-fetch": "2.6.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "svg-symbol-sprite-loader": "5.0.0"
  },
  "devDependencies": {
    "@crystal-ball/babel-base": "3.0.0",
    "@crystal-ball/commitizen-base": "2.10.0",
    "@crystal-ball/prettier-base": "2.6.0",
    "@crystal-ball/webpack-base": "10.2.0",
    "@types/jest": "26.0.20",
    "eslint-config-eloquence": "17.3.0",
    "husky": "4.3.8",
    "jest": "26.6.3"
  },
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
