{
  "name": "crystal-ball.github.io",
  "version": "1.0.0",
  "description": "Crystal Ball Projects resources and documentation ðŸ”®âœ¨",
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "keywords": [
    "crystal-ball",
    "awesomeness"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/crystal-ball.github.io.git"
  },
  "scripts": {
    "test": "npm run test:format && npm run test:lint && npm run test:unit",
    "test:format": "prettier --check '*.{js,json,md,json}' '**/*.{md,js,json,scss}'",
    "test:lint": "NODE_ENV=test eslint --format=pretty scripts",
    "test:unit": "jest scripts",
    "create-github-labels": "node ./scripts/create-github-labels.js",
    "format": "prettier --write '*.{js,json,md}' '**/*.{js,json,md,scss}'"
  },
  "dependencies": {
    "inquirer": "7.0.0",
    "node-fetch": "2.6.0"
  },
  "devDependencies": {
    "@crystal-ball/commitizen-base": "2.0.0",
    "@crystal-ball/prettier-base": "1.5.0",
    "@types/jest": "24.0.18",
    "eslint-config-eloquence": "12.5.0",
    "husky": "3.0.8",
    "jest": "24.9.0"
  },
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  },
  "prettier": "@crystal-ball/prettier-base"
}
