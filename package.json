{
  "name": "crystal-ball.github.io",
  "version": "1.0.0",
  "description": "Crystal Ball Projects resources and documentation ðŸ”®âœ¨",
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "keywords": [
    "crystal-ball",
    "awesomeness"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/crystal-ball.github.io.git"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode=development",
    "start:docker": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --mode=development",
    "build": "NODE_ENV=production webpack --mode=production",
    "test": "npm run test:lint && npm run test:unit",
    "test:lint": "NODE_ENV=test eslint --format=pretty --ext .js,.ts scripts src",
    "test:unit": "jest scripts",
    "create-github-labels": "node ./scripts/create-github-labels.js",
    "format": "prettier --write '*.{js,json,md,ts}' '**/*.{js,json,md,mdx,scss,ts}'"
  },
  "dependencies": {
    "@babel/runtime": "7.9.2",
    "@emotion/core": "10.0.28",
    "@mdx-js/react": "1.6.0",
    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "emotion-theming": "10.0.27",
    "feather-icons": "4.28.0",
    "inquirer": "7.1.0",
    "node-fetch": "2.6.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hot-loader": "4.12.20",
    "svg-symbol-sprite-loader": "5.0.0"
  },
  "devDependencies": {
    "@crystal-ball/babel-base": "1.6.1",
    "@crystal-ball/commitizen-base": "2.6.0",
    "@crystal-ball/prettier-base": "2.2.0",
    "@crystal-ball/webpack-base": "9.3.0",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@types/jest": "25.2.1",
    "eslint-config-eloquence": "13.8.0",
    "husky": "4.2.5",
    "jest": "26.0.1"
  },
  "prettier": "@crystal-ball/prettier-base",
  "config": {
    "commitizen": {
      "path": "@crystal-ball/commitizen-base"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "renovate": {
    "extends": [
      "github>crystal-ball/renovate-base:project"
    ]
  }
}
