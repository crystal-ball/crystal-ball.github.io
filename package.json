{
  "name": "crystal-ball.github.io",
  "version": "1.0.0",
  "description": "Organization shared resources and documentation",
  "contributors": [
    "Dan Hedgecock <hedgecock.d@gmail.com> (https://danhedgecock.com)"
  ],
  "keywords": [
    "crystal-ball",
    "awesomeness"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/crystal-ball/crystal-ball.github.io.git"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint *.js",
    "labels": "node ./scripts/github-labels.js",
    "format": "prettier --write \"*.{js,md}\" \"{.github,scripts}/**/*.{js,json,md}\""
  },
  "dependencies": {
    "inquirer": "6.3.1",
    "node-fetch": "2.4.1"
  },
  "devDependencies": {
    "@crystal-ball/commitizen-base": "1.0.0",
    "@crystal-ball/prettier-base": "1.0.0",
    "eslint-config-eloquence": "11.0.0",
    "husky": "2.1.0"
  },
  "prettier": "@crystal-ball/prettier-base",
  "config": {
    "commitizen": {
      "path": "cz-adapter-eslint"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook"
    }
  }
}